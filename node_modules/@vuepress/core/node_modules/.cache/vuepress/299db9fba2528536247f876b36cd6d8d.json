\r\n              </v-flex>\r\n            </v-layout>\r\n\r\n            <input type=\"radio\" name=\"example\" value=\"サンプル\">サンプル\r\n            <input type=\"radio\" name=\"example\" value=\"サンプル\">シンプル\r\n            \r\n            <v-layout row justify-center align-center mx-2>\r\n              <v-flex>\r\n              <v-radio-group v-model=\"GroupHS\" col>\r\n                <v-flex>\r\n                <v-radio\r\n                  label=\"ＨＳ\"\r\n                   value=\"1\"\r\n                 ></v-radio>\r\n                 </v-flex>\r\n                 <v-flex>\r\n                <v-radio\r\n                  label=\"ＨＳ\"\r\n                  value=\"2\"\r\n                ></v-radio>\r\n                </v-flex>\r\n                <v-flex>\r\n                <v-radio\r\n                  label=\"ＨＳ\"\r\n                   value=\"3\"\r\n                 ></v-radio>\r\n                 </v-flex>\r\n                 <v-flex>\r\n                <v-radio\r\n                  label=\"ＨＳ\"\r\n                  value=\"4\"\r\n                ></v-radio>\r\n                </v-flex>\r\n              </v-radio-group>\r\n              </v-flex>\r\n\r\n              <v-flex>\r\n              <v-radio-group v-model=\"GroupSUD\" col class=\"pa-1000 ma-1000\">\r\n                <v-radio\r\n                  label=\"サド\"\r\n                   value=\"1\"\r\n                 ></v-radio>\r\n                <v-radio\r\n                  label=\"サド\"\r\n                  value=\"2\"\r\n                ></v-radio>\r\n                <v-radio\r\n                  label=\"サド\"\r\n                   value=\"3\"\r\n                 ></v-radio>\r\n                <v-radio\r\n                  label=\"サド\"\r\n                  value=\"4\"\r\n                ></v-radio>\r\n              </v-radio-group>\r\n              </v-flex>\r\n  \r\n              <v-flex col>\r\n                <v-flex>\r\n                <v-checkbox\r\n                  v-model=\"onSud1\"\r\n                  label=\"サドあり\"\r\n                ></v-checkbox>\r\n                </v-flex>\r\n                <v-flex>\r\n                <v-checkbox\r\n                  v-model=\"onSud2\"\r\n                  label=\"サドあり\"\r\n                ></v-checkbox>\r\n                </v-flex>\r\n                <v-flex>\r\n                <v-checkbox\r\n                  v-model=\"onSud3\"\r\n                  label=\"サドあり\"\r\n                ></v-checkbox>\r\n                </v-flex>\r\n                <v-flex>\r\n                <v-checkbox\r\n                  v-model=\"onSud4\"\r\n                  label=\"サドあり\"\r\n                ></v-checkbox>\r\n                </v-flex>\r\n              </v-flex>\r\n    \r\n              <v-flex xs2 col>\r\n                <v-text-field\r\n                  v-model=\"BPM1\"\r\n                  type=\"number\"\r\n                  prefix=\"BPM：\"\r\n                ></v-text-field>\r\n                <v-text-field\r\n                  v-model=\"BPM2\"\r\n                  type=\"number\"\r\n                  prefix=\"BPM：\"\r\n                ></v-text-field>\r\n                <v-text-field\r\n                  v-model=\"BPM3\"\r\n                  type=\"number\"\r\n                  prefix=\"BPM：\"\r\n                ></v-text-field>\r\n                <v-text-field\r\n                  v-model=\"BPM4\"\r\n                  type=\"number\"\r\n                  prefix=\"BPM：\"\r\n                ></v-text-field>\r\n              </v-flex>\r\n            </v-layout>\r\n          </v-card>\r\n        </v-container>\r\n\r\n        <v-container>\r\n          <v-card>\r\n            <v-card-title class=\"title justify-center\">ハイスピ：{{lowest}}、サドプラ：{{lowest}}</v-card-title>\r\n            <v-layout row justify-center align-center>\r\n              <v-data-table\r\n                :headers=\"headers\"\r\n                :items=\"hySpeeds\"\r\n                class=\"elevation-1\"\r\n               >\r\n                <template v-slot:items=\"props\">\r\n                  <td>{{ props.item.name }}</td>\r\n                  <td class=\"text-xs-right\">{{ props.item.mag }}</td>\r\n                  <td class=\"text-xs-right\">{{ props.item.HS1 }}</td>\r\n                  <td class=\"text-xs-right\">{{ props.item.HS2 }}</td>\r\n                  <td class=\"text-xs-right\">{{ props.item.HS3 }}</td>\r\n                  <td class=\"text-xs-right\">{{ props.item.HS4 }}</td>\r\n                </template>\r\n              </v-data-table>\r\n            </v-layout>\r\n          </v-card>\r\n        </v-container>\r\n      </v-content>\r\n    </v-app>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    name: 'Sud',\r\n    \r\n    data() {\r\n      return {\r\n        headers: [\r\n          {\r\n            text: 'サドプラ',\r\n            align: 'left',\r\n            sortable: false,\r\n            value: 'name'\r\n          },\r\n          { text: '倍率', value: 'mag' },\r\n          { text: 'HS1', value: 'HS1' },\r\n          { text: 'HS2', value: 'HS2' },\r\n          { text: 'HS3', value: 'HS3' },\r\n          { text: 'HS4', value: 'HS4'}\r\n        ],\r\n        hySpeeds: [\r\n          {\r\n            name: '適サド',\r\n            mag: 2.1,\r\n            HS1: 640,\r\n            HS2: 630,\r\n            HS3: 640,\r\n            HS4: 320\r\n          },\r\n          \r\n        ]\r\n      }\r\n    },\r\n    computed: {\r\n      coinper: function () {\r\n        var ret\r\n        if (this.coin > 0) {\r\n          ret = Math.floor(100 * 10 / (3 * this.coin)) /10\r\n        }\r\n        return ret\r\n      },\r\n      poffper: function () {\r\n        var ret\r\n        if (this.poff > 0) {\r\n          ret = Math.floor(this.poff * 10 / 3) /10\r\n        }\r\n        return ret\r\n      },\r\n      ponper: function () {\r\n        var ret\r\n        if (this.pon > 0) {\r\n          ret = Math.floor(this.pon * 2 * 10 / 7) /10\r\n        }\r\n        return ret\r\n      },\r\n      lowest: function () {\r\n        var isInput = false\r\n        var temp = [this.poffper, this.coinper, this.ponper]\r\n        var prices = temp.map(function(value) {\r\n          if (value > 0) {\r\n            isInput = true\r\n            return value\r\n          }\r\n          return 999\r\n        })\r\n        if (isInput == false) {\r\n          return \"？\"\r\n        }\r\n        var key = prices.indexOf(Math.min.apply(null,prices))\r\n        var name = ['パセリ（特典なし）', '１００円', 'パセリ（特典あり）']\r\n        var ret = name[key]\r\n        return ret\r\n      }\r\n    }\r\n  }\r\n</script>"],"sourceRoot":"src/.vuepress/components"}]}